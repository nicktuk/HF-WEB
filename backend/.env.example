# Database
DATABASE_URL=postgresql://user:password@localhost:5432/catalog_db

# Security
ADMIN_API_KEY=generate_with_python_secrets_token_urlsafe_32
SECRET_KEY=generate_with_python_secrets_token_urlsafe_32

# CORS
ALLOWED_ORIGINS=["http://localhost:3000"]

# Environment
ENVIRONMENT=development

# Scraping
SCRAPER_TIMEOUT=30
SCRAPER_MAX_RETRIES=3

# Scheduler
SCRAPE_PRODUCTS_CRON=0 3 * * *
UPDATE_MARKET_PRICES_CRON=0 */12 * * *

# Cache TTL (seconds)
CACHE_TTL_PRODUCTS=300
CACHE_TTL_MARKET_PRICES=43200

# Rate Limiting
RATE_LIMIT_PUBLIC=100/minute
RATE_LIMIT_ADMIN=1000/minute
